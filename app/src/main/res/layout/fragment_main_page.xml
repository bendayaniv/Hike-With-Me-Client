<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainPage_fragment"
    android:name="com.google.android.gms.maps.SupportMapFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="15dp"
    tools:context=".Controller.Fragments.MainActivityFragments.MainPage.MapsFragment">

    <FrameLayout
        android:id="@+id/mainPageMapFragment"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:padding="5dp" />

    <View
        android:id="@+id/mainPage_VIEW_break"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_below="@id/mainPageMapFragment"
        />

    <FrameLayout
        android:id="@+id/mainPageRoutesListFragment"
        android:layout_width="match_parent"
        android:layout_height="350dp"
        android:layout_below="@id/mainPage_VIEW_break"
        android:padding="5dp" />

</androidx.appcompat.widget.LinearLayoutCompat>
